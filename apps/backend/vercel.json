{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "api/index.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "buildCommand": "",
  "outputDirectory": "",
  "crons": [
    {
      "path": "/api/trigger-campaign-executor",
      "schedule": "* * * * *"
    }
  ]
}
