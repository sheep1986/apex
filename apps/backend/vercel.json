{
  "version": 2,
  "functions": {
    "server-simple.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server-simple.js"
    }
  ],
  "crons": [
    {
      "path": "/api/trigger-campaign-executor",
      "schedule": "* * * * *"
    }
  ]
}
